<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="ECMA">
<description>Provides a scanner for ECMA script files.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptFileDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptVariableDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptFunctionDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptFunctionParameterDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptObjectDescriptor</class>

		<class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptArrayDescriptor</class>
		<class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptObjectDescriptor</class>
		
		<class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScripArrayDeclares</class>
		<class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScripObjectDeclares</class> 

        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptLiteralDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptNullDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptStringDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptNumberDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptBooleanDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptUndefinedDescriptor</class>

		<class>org.jqassistant.contrib.plugin.ecmascript.api.model.ECMAScriptClassDescriptor</class>

    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.ecmascript.api.scanner.ECMAScriptFileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>