<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="JS">
<description>Provides a scanner for Javascript files.</description>
    <model>
    	<class>org.jqassistant.contrib.plugin.javascript.api.model.JsDescriptor</class>
    
    	<class>org.jqassistant.contrib.plugin.javascript.api.model.ArrayDescriptor</class>
		<class>org.jqassistant.contrib.plugin.javascript.api.model.ObjectDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.javascript.api.model.ObjectDeclaresRelationshipDescriptor</class>
		<class>org.jqassistant.contrib.plugin.javascript.api.model.ArrayDeclaresRelationshipDescriptor</class>
    
        <class>org.jqassistant.contrib.plugin.javascript.api.model.JsFileDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javascript.api.model.VariableDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javascript.api.model.FunctionDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javascript.api.model.FunctionParameterDescriptor</class>

		
        <class>org.jqassistant.contrib.plugin.javascript.api.model.LiteralDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javascript.api.model.NullDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javascript.api.model.StringDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javascript.api.model.NumberDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javascript.api.model.BooleanDescriptor</class>
        <class>org.jqassistant.contrib.plugin.javascript.api.model.UndefinedDescriptor</class>

		<class>org.jqassistant.contrib.plugin.javascript.api.model.ClassDescriptor</class>

    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.javascript.api.scanner.JsFileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>